<?xml version="1.0" encoding="UTF-8"?>
<cronentries>
    <cron>
        <url>/api/polling-latest-gradle-stable.task.groovy</url>
        <description>Check the current version and trigger events if it has been changed</description>
        <schedule>every 1 hours</schedule>
        <retry-parameters>
            <min-backoff-seconds>30</min-backoff-seconds>
            <max-doublings>3</max-doublings>
        </retry-parameters>
    </cron>
    <cron>
        <url>/api/polling-latest-gradle-rc.task.groovy</url>
        <description>Check the current version and trigger events if it has been changed</description>
        <schedule>every 1 hours</schedule>
        <retry-parameters>
            <min-backoff-seconds>30</min-backoff-seconds>
            <max-doublings>3</max-doublings>
        </retry-parameters>
    </cron>
</cronentries>